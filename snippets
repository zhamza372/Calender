ef method1(a, b=1, *c)
  puts "a: #{a}, b: #{b}, c: #{c}"
end

def method2(a, *c, b)
  puts "a: #{a}, b: #{b}, c: #{c}"
end

def method3(a, *b, c=1)
  puts "a: #{a}, b: #{b}, c: #{c}"
end

method1(1,2,3,4)
method2(1,2,3,4)
method3(1,2,3,4)
